@Article{Walsh2018,
  author                     = {Walsh, Jake R. and Pedersen, Eric J. and Vander Zanden, M. Jake},
  title                      = {{Detecting species at low densities: a new theoretical framework and an empirical test on an invasive zooplankton}},
  journal                    = {{ECOSPHERE}},
  year                       = {{2018}},
  volume                     = {{9}},
  number                     = {{11}},
  month                      = {{NOV}},
  abstract                   = {{Species often occur at low abundance and, as a result, may evade
   detection during sampling. Here, we develop a general theoretical model
   of how the probability of detecting a species in a discrete sample
   varies with its mean density in that location based on sampling
   statistics. We show that detection probability at low densities can be
   approximated with a simple one parameter saturating function: The
   probability of detection (P) should increase with the mean number of
   individuals in a discrete sample (N) as P similar to alpha N/(1 + alpha
   N). We further show how the parameter alpha will be affected by species
   spatial aggregation, within-sample observability, and gear catchability.
   We use the case of the invasive zooplankter, spiny water flea
   (Bythotrephes longimanus), to demonstrate that this theoretical model
   fits the empirical pattern of detectability. In Lake Mendota, WI,
   Bythotrephes went undetected despite rigorous long-term zooplankton
   monitoring (possibly as long as 14 yr with 15 sampling dates/yr). Using
   our modeling framework, we found that the likelihood of detecting
   Bythotrephes was low unless peak annual densities exceeded 0.1
   individuals/m(3) over multiple years. Despite using models based on
   Bythotrephes ecology to identify ways to increase detectability,
   detection probabilities remained low at low densities, such that cases
   of missed detection such as Lake Mendota are possible despite rigorous
   monitoring effort. As such, our theoretical model provides a simple rule
   of thumb for estimating sampling effort required to detect rare species
   when densities are expected to be low: The minimum samples required (S)
   to reliably detect a species can be approximated as S similar to(1 +
   N)/N.}},
  address                    = {{111 RIVER ST, HOBOKEN 07030-5774, NJ USA}},
  affiliation                = {{Walsh, JR (Reprint Author), Univ Wisconsin, Ctr Limnol, 680 N Pk St, Madison, WI 53706 USA. Walsh, Jake R.; Pedersen, Eric J.; Vander Zanden, M. Jake, Univ Wisconsin, Ctr Limnol, 680 N Pk St, Madison, WI 53706 USA. Pedersen, Eric J., Fisheries \& Oceans Canada, Northwest Atlantic Fisheries Ctr, St John, NF, Canada.}},
  article-number             = {{e02475}},
  author-email               = {{jransom.walsh@gmail.com}},
  da                         = {{2019-05-23}},
  doc-delivery-number        = {{HD6DA}},
  doi                        = {{10.1002/ecs2.2475}},
  funding-acknowledgement    = {{Wisconsin Department of Natural Resources; NSF North Temperate Lakes Long Term Ecological Research Program {[}DEB-0217533, DEB-1440297]}},
  funding-text               = {{Thanks to Jared Burris, Carly Broshat, Steve McMahon, Evan Booth, Ryan Toman, Cristin Fitzpatrick, Sam Christel, Petra Wakker, and Olin Halsten for stellar work in the laboratory and field. Scott Van Egeren and Maureen Ferry for suggestions and advice in designing and applying this study. Thanks to Ali Mikulyuk and Tyler Tunney for comments. This work was funded by the Wisconsin Department of Natural Resources, and the NSF North Temperate Lakes Long Term Ecological Research Program (grants DEB-0217533 and DEB-1440297). JRW and MJVZ designed research. JRW performed the field sampling program. JRW, EJP, and MJVZ analyzed data. EJP developed theoretical framework and JRW, EJP, and MJVZ wrote the paper.}},
  issn                       = {{2150-8925}},
  journal-iso                = {{Ecosphere}},
  keywords                   = {{Bythotrephes longimanus; detection; invasive species; zooplankton}},
  keywords-plus              = {{SPINY WATER FLEA; BYTHOTREPHES-LONGIMANUS; SURVEILLANCE SYSTEMS; ESTIMATING ABUNDANCE; NORTH-AMERICA; IMPACTS; HABITAT; SPREAD; ESTABLISHMENT; OCCUPANCY}},

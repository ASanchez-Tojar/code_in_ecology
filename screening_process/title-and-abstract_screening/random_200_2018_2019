@Article{Staver2019,
  author                     = {Staver, A. Carla and Wigley-Coetsee, Corli and Botha, Judith},
  title                      = {{Grazer movements exacerbate grass declines during drought in an African savanna}},
  journal                    = {{JOURNAL OF ECOLOGY}},
  year                       = {{2019}},
  volume                     = {{107}},
  number                     = {{3}},
  pages                      = {{1482-1491}},
  month                      = {{MAY}},
  abstract                   = {{Rainfall variability, including drought, consistently translates into
   variation in grass productivity in savanna ecosystems. These variations
   in grass availability can impact grazer populations, which can in turn
   shape grass responses to drought. However, despite clear evidence that
   grazers move freely in response to forage and water availability,
   drought ecologists have mostly failed to consider how these
   landscape-scale processes influence vegetation responses to drought.
   Here, we ask how rainfall variability and drought, in interaction with
   population dynamics and movement of large grazers, shape grass layer
   responses in extant savanna landscapes in Kruger National Park in South
   Africa. We found that grass biomass was highly responsive to rainfall
   variability; notably, grass declines during drought were especially
   severe-even more severe than previously documented elsewhere. Grazing
   probably contributed, exacerbating drought effects in droughted regions
   and spatially extending them into non-droughted refugia, where grass
   biomass also declined. Moreover, the existence of these refugia
   potentially prevented a grazer population crash. Synthesis. Our current
   focus on the physiological effects of drought is probably insufficient
   for understanding vegetation responses to drought. A renewed focus on
   landscape processes, including animal movement, will be critical to
   predicting savanna responses to increasingly frequent extreme events.}},
  address                    = {{111 RIVER ST, HOBOKEN 07030-5774, NJ USA}},
  affiliation                = {{Staver, AC (Reprint Author), Yale Univ, Ecol \& Evolutionary Biol, New Haven, CT 06520 USA. Staver, A. Carla, Yale Univ, Ecol \& Evolutionary Biol, New Haven, CT 06520 USA. Wigley-Coetsee, Corli; Botha, Judith, South African Natl Pk, Sci Serv, Skukuza, South Africa.}},
  author-email               = {{carla.staver@yale.edu}},
  da                         = {{2019-05-23}},
  doc-delivery-number        = {{HX5CG}},
  doi                        = {{10.1111/1365-2745.13106}},
  eissn                      = {{1365-2745}},
  issn                       = {{0022-0477}},
  journal-iso                = {{J. Ecol.}},
  keywords                   = {{climate extremes; drought; drought refugia; grass; grazer; migration; movement; savanna}},
  keywords-plus              = {{FIRE; LANDSCAPE; RAINFALL; HERBIVORES; MANAGEMENT; SCALE; CONSEQUENCES; VARIABILITY; SENSITIVITY; VEGETATION}},
  language                   = {{English}},
  number-of-cited-references = {{59}},
  publisher                  = {{WILEY}},
  research-areas             = {{Plant Sciences; Environmental Sciences \& Ecology}},
  times-cited                = {{0}},
  type                       = {{Article}},

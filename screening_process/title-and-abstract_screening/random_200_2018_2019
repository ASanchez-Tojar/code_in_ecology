@Article{Nord2019,
  author                     = {Nord, Andreas and Nilsson, Jan-Ake},
  title                      = {{Heat dissipation rate constrains reproductive investment in a wild bird}},
  journal                    = {{FUNCTIONAL ECOLOGY}},
  year                       = {{2019}},
  volume                     = {{33}},
  number                     = {{2}},
  pages                      = {{250-259}},
  month                      = {{FEB}},
  abstract                   = {{The heat dissipation limit theory (HDL) posits that animals with higher
   capacity to dissipate metabolic heat can increase reproductive
   investment. This theory remains untested in the wild. We recently showed
   that increased workload in a small bird causally relates to maximum body
   temperature. Here, we have expanded this approach by experimentally
   facilitating sensible heat transfer rate in nestling-feeding blue titsa
   small bird with high resting- and work-induced body temperaturesthrough
   removal of ventral plumage. Feather-clipped parents did not increase
   work rate but sired larger, and sometimes heavier, nestlings while
   maintaining lower body temperature and losing less body mass than
   controls. Thus, when relieved of the demands to dissipate metabolic
   heat, parents could invest more into both current (nestling condition)
   and future (self-maintenance) reproduction. In accordance with the HDL
   theory, we conclude that constraints on heat dissipation rate could be a
   potent mediator of life-history trade-offs in wild animals.}},
  address                    = {{111 RIVER ST, HOBOKEN 07030-5774, NJ USA}},
  affiliation                = {{Nord, A (Reprint Author), Lund Univ, Dept Biol, Sect Evolutionary Ecol, Ecol Bldg, Lund, Sweden. Nord, A (Reprint Author), Univ Glasgow, Scottish Ctr Ecol \& Nat Environm, Rowardennan, Scotland. Nord, Andreas; Nilsson, Jan-Ake, Lund Univ, Dept Biol, Sect Evolutionary Ecol, Ecol Bldg, Lund, Sweden. Nord, Andreas, Univ Glasgow, Scottish Ctr Ecol \& Nat Environm, Rowardennan, Scotland.}},
  author-email               = {{andreas.nord@biol.lu.se}},
  da                         = {{2019-05-23}},
  doc-delivery-number        = {{HL6FZ}},
  doi                        = {{10.1111/1365-2435.13243}},
  eissn                      = {{1365-2435}},

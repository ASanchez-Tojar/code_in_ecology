@Article{Basham2019,
  author                     = {Basham, Edmund W. and Seidl, Christa M. and Andriamahohatra, Lydou R. and Oliveira, Brunno F. and Scheffers, Brett R.},
  title                      = {{Distance-decay differs among vertical strata in a tropical rainforest}},
  journal                    = {{JOURNAL OF ANIMAL ECOLOGY}},
  year                       = {{2019}},
  volume                     = {{88}},
  number                     = {{1}},
  pages                      = {{114-124}},
  month                      = {{JAN}},
  abstract                   = {{Assemblage similarity decays with geographic distance-a pattern known as
   the distance-decay relationship. While this pattern has been
   investigated for a wide range of organisms, ecosystems and geographical
   gradients, whether these changes vary more cryptically across different
   forest strata (from ground to canopy) remains elusive. Here, we
   investigated the influence of ground vs. arboreal assemblages to the
   general distance-decay relationship observed in forests. We seek to
   explain differences in distance-decay relationships between strata in
   the context of the vertical stratification of assemblage composition,
   richness and abundance. We surveyed for a climate-sensitive model
   organism, amphibians, across vertical rainforest strata in Madagascar.
   For each tree, we defined assemblages of ground-dwelling, understory, or
   canopy species. We calculated horizontal distance-decay in similarity
   across all trees, and across assemblages of species found in different
   forest strata (ground, understory and canopy). We demonstrate that
   within stratum comparisons exhibit a classic distance-decay relationship
   for canopy and understory communities but no distance-decay
   relationships for ground communities. We suggest that differences in
   horizontal turnover between strata may be due to local scale habitat and
   resource heterogeneity in the canopy, or the influence of arboreal
   traits on species dispersal and distribution. Synthesis. Biodiversity
   patterns in horizontal space were not consistent across vertical space,
   suggesting that canopy fauna may not play by the same set of ``rules{''}
   as their conspecifics living below them on the ground. Our study
   provides compelling evidence that the above-ground amphibian assemblage
   of tropical rainforests is the primary driver of the classical
   distance-decay relationship.}},
  address                    = {{111 RIVER ST, HOBOKEN 07030-5774, NJ USA}},
  affiliation                = {{Basham, EW; Scheffers, BR (Reprint Author), Univ Florida, Sch Nat Resources \& Environm, Gainesville, FL 32611 USA. Scheffers, BR (Reprint Author), Univ Florida, Dept Wildlife Ecol \& Conservat, Gainesville, FL 32611 USA. Basham, Edmund W.; Scheffers, Brett R., Univ Florida, Sch Nat Resources \& Environm, Gainesville, FL 32611 USA. Seidl, Christa M., Univ Calif Santa Cruz, Dept Ecol \& Evolutionary Biol, Santa Cruz, CA 95064 USA. Andriamahohatra, Lydou R., Univ Antananarivo, Ecole Normale Super, CER Nat Sci, Dept Expt Sci, Antananarivo, Madagascar. Oliveira, Brunno F.; Scheffers, Brett R., Univ Florida, Dept Wildlife Ecol \& Conservat, Gainesville, FL 32611 USA.}},
  author-email               = {{ebasham1@ufl.edu brett.scheffers@ufl.edu}},
  da                         = {{2019-05-23}},
  doc-delivery-number        = {{HI8FN}},
  doi                        = {{10.1111/1365-2656.12902}},
  eissn                      = {{1365-2656}},
  funding-acknowledgement    = {{National Geographic Society {[}9480-14]}},

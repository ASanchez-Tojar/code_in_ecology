@Article{Marcionetti2018,
  author                     = {Marcionetti, Anna and Rossier, Victor and Bertrand, Joris A. M. and Litsios, Glenn and Salamin, Nicolas},
  title                      = {{First draft genome of an iconic clownfish species (Amphiprion frenatus)}},
  journal                    = {{MOLECULAR ECOLOGY RESOURCES}},
  year                       = {{2018}},
  volume                     = {{18}},
  number                     = {{5}},
  pages                      = {{1092-1101}},
  month                      = {{SEP}},
  abstract                   = {{Clownfishes (or anemonefishes) form an iconic group of coral reef
   fishes, principally known for their mutualistic interaction with sea
   anemones. They are characterized by particular life history traits, such
   as a complex social structure and mating system involving sequential
   hermaphroditism, coupled with an exceptionally long lifespan.
   Additionally, clownfishes are considered to be one of the rare groups to
   have experienced an adaptive radiation in the marine environment. Here,
   we assembled and annotated the first genome of a clownfish species, the
   tomato clownfish (Amphiprion frenatus). We obtained 17,801 assembled
   scaffolds, containing a total of 26,917 genes. The completeness of the
   assembly and annotation was satisfying, with 96.5\% of the
   Actinopterygii Benchmarking Universal Single-Copy Orthologs (BUSCOs)
   being retrieved in A.frenatus assembly. The quality of the resulting
   assembly is comparable to other bony fish assemblies. This resource is
   valuable for advancing studies of the particular life history traits of
   clownfishes, as well as being useful for population genetic studies and
   the development of new phylogenetic markers. It will also open the way
   to comparative genomics. Indeed, future genomic comparison among closely
   related fishes may provide means to identify genes related to the unique
   adaptations to different sea anemone hosts, as well as better
   characterize the genomic signatures of an adaptive radiation.}},
  address                    = {{111 RIVER ST, HOBOKEN 07030-5774, NJ USA}},
  affiliation                = {{Salamin, N (Reprint Author), Univ Lausanne, Biophore, Dept Computat Biol, Lausanne, Switzerland. Marcionetti, Anna; Rossier, Victor; Bertrand, Joris A. M.; Litsios, Glenn; Salamin, Nicolas, Univ Lausanne, Biophore, Dept Computat Biol, Lausanne, Switzerland. Marcionetti, Anna; Rossier, Victor; Bertrand, Joris A. M.; Litsios, Glenn; Salamin, Nicolas, Swiss Inst Bioinformat, Lausanne, Switzerland.}},
  author-email               = {{nicolas.salamin@unil.ch}},
  da                         = {{2019-05-23}},
  doc-delivery-number        = {{GQ5SZ}},
  doi                        = {{10.1111/1755-0998.12772}},
  eissn                      = {{1755-0998}},
  funding-acknowledgement    = {{Universite de Lausanne; Schweizerischer Nationalfonds zur Forderung der Wissenschaftlichen Forschung {[}31003A-163428]}},
  funding-text               = {{Universite de Lausanne; Schweizerischer Nationalfonds zur Forderung der Wissenschaftlichen Forschung, Grant/Award Number: 31003A-163428}},

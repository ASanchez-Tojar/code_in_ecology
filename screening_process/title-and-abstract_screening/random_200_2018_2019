  issn                       = {{0962-8452}},
  journal-iso                = {{Proc. R. Soc. B-Biol. Sci.}},
  keywords                   = {{inbreeding; decision-making; resource availability; burying beetle; fitness consequences}},
  keywords-plus              = {{INBREEDING DEPRESSION; CLUTCH SIZE; BROOD SIZE; BODY-SIZE; RESOURCE AVAILABILITY; OFFSPRING FITNESS; EGG SIZE; NICROPHORUS; WILD; EXPERIENCE}},

@Article{Richardson2018,
  author                     = {Richardson, Graham and Dickinson, Patrick and Burman, Oliver H. P. and Pike, Thomas W.},
  title                      = {{Unpredictable movement as an anti-predator strategy}},
  journal                    = {{PROCEEDINGS OF THE ROYAL SOCIETY B-BIOLOGICAL SCIENCES}},
  year                       = {{2018}},
  volume                     = {{285}},
  number                     = {{1885}},
  month                      = {{AUG 29}},
  abstract                   = {{Prey animals have evolved a wide variety of behaviours to combat the
   threat of predation, and these have been generally well studied.
   However, one of the most common and taxonomically widespread
   antipredator behaviours of all has, remarkably, received almost no
   experimental attention: so-called `protean' behaviour. This is behaviour
   that is sufficiently unpredictable to prevent a predator anticipating in
   detail the future position or actions of its prey. In this study, we
   used human `predators' participating in 3D virtual reality simulations
   to test how protean (i.e. unpredictable) variation in prey movement
   affects participants' ability to visually target them as they move (a
   key determinant of successful predation). We found that targeting
   accuracy was significantly predicted by prey movement path complexity,
   although, surprisingly, there was little evidence that high levels of
   unpredictability in the underlying movement rules equated directly to
   decreased predator performance. Instead, the specific movement rules
   differed in how they impacted on targeting accuracy, with the efficacy
   of protean variation in one element depending on the values of the
   remaining elements. These findings provide important insights into the
   understudied phenomenon of protean antipredator behaviour, which are
   directly applicable to predator-prey dynamics within a broad range of
   taxa.}},
  address                    = {{6-9 CARLTON HOUSE TERRACE, LONDON SW1Y 5AG, ENGLAND}},
  affiliation                = {{Pike, TW (Reprint Author), Univ Lincoln, Sch Life Sci, Lincoln LN6 7DL, England. Richardson, Graham; Burman, Oliver H. P.; Pike, Thomas W., Univ Lincoln, Sch Life Sci, Lincoln LN6 7DL, England. Richardson, Graham; Dickinson, Patrick, Univ Lincoln, Sch Comp Sci, Lincoln LN6 7TS, England.}},
  article-number             = {{20181112}},
  author-email               = {{tpike@lincoln.ac.uk}},
  da                         = {{2019-05-23}},
  doc-delivery-number        = {{GS0FM}},
  doi                        = {{10.1098/rspb.2018.1112}},
  eissn                      = {{1471-2954}},
  funding-acknowledgement    = {{School of Life Sciences, University of Lincoln}},
  funding-text               = {{G.R. was supported by a scholarship from the School of Life Sciences, University of Lincoln.}},
  issn                       = {{0962-8452}},
  journal-iso                = {{Proc. R. Soc. B-Biol. Sci.}},
  keywords                   = {{predator-prey interactions; anti-predator defence; unpredictability; virtual reality}},
  keywords-plus              = {{CORRELATED RANDOM-WALK; PRAYING-MANTIS; PREDATION RISK; BEHAVIOR; FLIGHT; PREY; AVOIDANCE; BIOMECHANICS; DRAGONFLIES; TRACKING}},

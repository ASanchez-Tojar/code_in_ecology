@Article{Meisel2018,
  author                     = {Meisel, Richard P.},
  title                      = {{Sexual conflict and the maintenance of genetic variation in natural populations}},
  journal                    = {{MOLECULAR ECOLOGY}},
  year                       = {{2018}},
  volume                     = {{27}},
  number                     = {{18}},
  pages                      = {{3569-3571}},
  month                      = {{SEP}},
  abstract                   = {{Understanding the factors that maintain genetic variation in natural
   populations is a foundational goal of evolutionary biology. To this end,
   population geneticists have developed a variety of models that can
   produce stable polymorphisms. In one of the earliest models, Owen (1953)
   demonstrated that differences in selection pressures acting on males and
   females could maintain multiple alleles of a gene at a stable
   equilibrium. If the selection pressures act in opposite directions in
   males and females, we refer to this as (inter-) sexual conflict or
   sexual antagonism (Arnqvist \& Rowe, 2005). Testing if sexual conflict
   maintains genetic variation in natural populations is a tremendous
   challengeit requires both identifying loci that harbor sexually
   antagonistic alleles and determining whether those alleles are
   maintained as stable polymorphisms (Mank, 2017). Doing so genome-wide is
   even harder because it is not tractable to identify sexually
   antagonistic alleles and test for stable polymorphisms at all loci.
   Dutoit etal. (2018) confront this challenge in a paper published in this
   issue of Molecular Ecology. Using gene expression and population genomic
   data from the collared flycatcher, Dutoit etal. (2018) identify
   associations and correlations between genomic signatures of balanced
   polymorphisms and sexual conflict.}},
  address                    = {{111 RIVER ST, HOBOKEN 07030-5774, NJ USA}},
  affiliation                = {{Meisel, RP (Reprint Author), Univ Houston, Dept Biol \& Biochem, 3455 Cullen Blvd 342, Houston, TX 77204 USA. Meisel, Richard P., Univ Houston, Dept Biol \& Biochem, 3455 Cullen Blvd 342, Houston, TX 77204 USA.}},
  author-email               = {{rpmeisel@uh.edu}},
  da                         = {{2019-05-23}},
  doc-delivery-number        = {{GT5VC}},
  doi                        = {{10.1111/mec.14787}},
  eissn                      = {{1365-294X}},
  issn                       = {{0962-1083}},
  journal-iso                = {{Mol. Ecol.}},
  keywords-plus              = {{STABLE EQUILIBRIA; SELECTION; EVOLUTION; SEXES}},
  language                   = {{English}},
  number-of-cited-references = {{12}},
  orcid-numbers              = {{Meisel, Richard/0000-0002-7362-9307}},
  publisher                  = {{WILEY}},
  research-areas             = {{Biochemistry \& Molecular Biology; Environmental Sciences \& Ecology; Evolutionary Biology}},
  times-cited                = {{1}},
  type                       = {{Editorial Material}},

@Article{He2018b,
  author                     = {He, Nianpeng and Liu, Congcong and Tian, Miao and Li, Meiling and Yang, Hao and Yu, Guirui and Guo, Dali and Smith, Melinda D. and Yu, Qiang and Hou, Jihua},
  title                      = {{Variation in leaf anatomical traits from tropical to cold-temperate forests and linkage to ecosystem functions}},
  journal                    = {{FUNCTIONAL ECOLOGY}},
  year                       = {{2018}},
  volume                     = {{32}},
  number                     = {{1, SI}},
  pages                      = {{10-19}},
  month                      = {{JAN}},
  abstract                   = {{Leaf anatomical traits may reflect plants adaption to environmental
   changes and influence ecosystem functions, as they regulate light
   absorption and gas exchange to some extent. Here, we hypothesized that
   leaf anatomical traits were closely related to gross primary
   productivity (GPP) because photosynthesis commonly occurs in the
   chloroplasts of palisade and spongy tissues in leaf. Eight leaf
   anatomical traits were measured in 916 plant species inhabiting from
   tropical to cold-temperate forests in eastern China: adaxial epidermis
   thickness (AD), abaxial epidermis thickness (AB), leaf thickness (LT),
   palisade tissue thickness (PT), and spongy tissue thickness (ST),
   palisade-spongy tissue ratio (PT/ST), palisade tissue-leaf thickness
   ratio (PT/LT), and spongy tissue-leaf thickness ratio (ST/LT). Leaf
   anatomical traits showed significant latitudinal patterns at species,
   plant functional group (PFG), and community levels (p<.05), and they
   differed between PFG and community. Temperature and precipitation were
   the main factors influencing AD, AB, PT/ST, and PT/LT, explaining
   33-72\% of the total variation at large scale. Furthermore, AB, LT,
   PT/ST, and PT/LT were significantly correlated with the aridity index.
   Our findings filled the data gap of plant anatomical traits at regional
   scales, and broadened current knowledge on the adaptation strategies of
   plant anatomical traits, which also provided new evidence for linkages
   of plant traits and functioning across natural communities.
   A is available for this article.}},
  address                    = {{111 RIVER ST, HOBOKEN 07030-5774, NJ USA}},
  affiliation                = {{He, NP (Reprint Author), Chinese Acad Sci, Inst Geog Sci \& Nat Resources Res, Key Lab Ecosyst Network Observat \& Modeling, Beijing, Peoples R China. He, NP (Reprint Author), Univ Chinese Acad Sci, Coll Resources \& Environm, Beijing, Peoples R China. Hou, JH (Reprint Author), Beijing Forestry Univ, Key Lab Forest Resources \& Ecosyst Proc Beijing, Beijing, Peoples R China. He, Nianpeng; Liu, Congcong; Tian, Miao; Li, Meiling; Yang, Hao; Yu, Guirui; Guo, Dali, Chinese Acad Sci, Inst Geog Sci \& Nat Resources Res, Key Lab Ecosyst Network Observat \& Modeling, Beijing, Peoples R China. He, Nianpeng, Univ Chinese Acad Sci, Coll Resources \& Environm, Beijing, Peoples R China. Smith, Melinda D., Colorado State Univ, Dept Biol, Ft Collins, CO 80523 USA. Smith, Melinda D., Colorado State Univ, Grad Degree Program Ecol, Ft Collins, CO 80523 USA. Yu, Qiang, Chinese Acad Agr Sci, Inst Agr Resources \& Reg Planning, Natl Hulunber Grassland Ecosyst Observat \& Res St, Beijing, Peoples R China. Hou, Jihua, Beijing Forestry Univ, Key Lab Forest Resources \& Ecosyst Proc Beijing, Beijing, Peoples R China.}},
  author-email               = {{henp@igsnrr.ac.cn houjihua@bjfu.edu.cn}},
  da                         = {{2019-05-23}},
  doc-delivery-number        = {{FS0WN}},

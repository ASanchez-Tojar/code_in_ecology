@Article{Hahn2019,
  author                     = {Hahn, Philip G. and Agrawal, Anurag A. and Sussman, Kira I. and Maron, John L.},
  title                      = {{Population Variation, Environmental Gradients, and the Evolutionary Ecology of Plant Defense against Herbivory}},
  journal                    = {{AMERICAN NATURALIST}},
  year                       = {{2019}},
  volume                     = {{193}},
  number                     = {{1}},
  pages                      = {{20-34}},
  month                      = {{JAN 1}},
  abstract                   = {{A central tenet of plant defense theory is that adaptation to the
   abiotic environment sets the template for defense strategies, imposing a
   trade-off between plant growth and defense. Yet this trade-off, commonly
   found among species occupying divergent resource environments, may not
   occur across populations of single species. We hypothesized that more
   favorable climates and higher levels of herbivory would lead to
   increases in growth and defense across plant populations. We evaluated
   whether plant growth and defense traits covaried across 18 populations
   of showy milkweed (Asclepias speciosa) inhabiting an east-west climate
   gradient spanning 25 degrees of longitude. A suite of traits impacting
   defense (e.g., latex, cardenolides), growth (e.g., size), or both (e.g.,
   specific leaf area {[}SLA], trichomes) were measured in natural
   populations and in a common garden, allowing us to evaluate plastic and
   genetically based variation in these traits. In natural populations,
   herbivore pressure increased toward warmer sites with longer growing
   seasons. Growth and defense traits showed strong clinal patterns and
   were positively correlated. In a common garden, clines with climatic
   origin were recapitulated only for defense traits. Correlations between
   growth and defense traits were also weaker and more negative in the
   common garden than in the natural populations. Thus, our data suggest
   that climatically favorable sites likely facilitate the evolution of
   greater defense at minimal costs to growth, likely because of increased
   resource acquisition.}},
  address                    = {{1427 E 60TH ST, CHICAGO, IL 60637-2954 USA}},
  affiliation                = {{Hahn, PG (Reprint Author), Univ Montana, Div Biol Sci, Missoula, MT 59812 USA. Hahn, Philip G.; Sussman, Kira I.; Maron, John L., Univ Montana, Div Biol Sci, Missoula, MT 59812 USA. Agrawal, Anurag A., Cornell Univ, Dept Ecol \& Evolutionary Biol, Ithaca, NY 14853 USA.}},
  author-email               = {{phil.hahn@mso.umt.edu}},
  da                         = {{2019-05-23}},
  doc-delivery-number        = {{HF0XZ}},
  doi                        = {{10.1086/700838}},
  eissn                      = {{1537-5323}},
  funding-acknowledgement    = {{MPG Ranch; National Science Foundation (NSF) Established Program to Stimulate Competitive Research (EPSCoR) undergraduate fellowship; NSF {[}DEB-1553518, DEB-1619885]}},
  funding-text               = {{We thank Amy Hastings, Kay Hajek, and Ron White for assistance with cardenolide analysis. Meghan Fitzgerald provided field assistance and coordination in Washington. We thank Philip Ramsey and staff at the MPG Ranch (https://www.mpgranch.com/) for facilitating the common-garden experiment. The Columbia Basin Wildlife Refuge and Bob and Ester Masselink provided access to field sites. We are grateful to the editors and reviewers for providing insightful comments on the manuscript. John Bishop, Jennifer Thaler, and Jesse Miller provided helpful suggestions on early versions of the manuscript. MPG Ranch provided postdoctoral funding and equipment support for P.G.H. K.I.S. was supported by a National Science Foundation (NSF) Established Program to Stimulate Competitive Research (EPSCoR) undergraduate fellowship. A.A.A. was supported by NSF DEB-1619885 and a sabbatical leave from Cornell University. J.L.M. was supported by NSF DEB-1553518.}},
  issn                       = {{0003-0147}},
  journal-iso                = {{Am. Nat.}},

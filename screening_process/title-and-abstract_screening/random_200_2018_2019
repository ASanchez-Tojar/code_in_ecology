@Article{Skoeld2018,
  author                     = {Skoeld, Martin and Knape, Jonas},
  title                      = {{Bounding reproductive rates in state-space models for animal population dynamics}},
  journal                    = {{ECOSPHERE}},
  year                       = {{2018}},
  volume                     = {{9}},
  number                     = {{5}},
  month                      = {{MAY}},
  abstract                   = {{Time-series models applied in the study of animal population dynamics
   commonly assume linearity on the log-scale, leading to log-normally
   distributed rates of increase. While this is often computationally
   convenient, in particular when performing statistical inference in the
   presence of observation error, it may lead to unrealistic predictions
   for animals with a limited reproduction. We introduce a model that
   includes an explicit bound on the reproductive rate of an individual,
   and apply this to a population time series of ungulates in Kruger
   National Park, South Africa. Due to observational error, the
   year-to-year increases in animal counts occasionally exceeded the
   maximal reproductive rate of the animals. In such cases, the traditional
   unbounded model showed a tendency of overfitting data, leading to
   unrealistic predictions of the underlying population increase. An
   observed increase above the maximal reproductive rate also provides
   empirical confirmation that observation error exists. The model with an
   explicit bound was able to utilize this in order to separate
   observational error from population process noise, which the traditional
   unbounded model was unable to do. We conclude that enforcing a strict
   upper bound on the reproductive rate of an animal population model may
   lead to more realistic statistical inference than commonly applied
   log-linear models when an explicit bound on reproductive rate is known.
   We further conclude that introducing a bound on reproduction can greatly
   assist in separating observational error and population process noise
   for slow life histories, or more generally, when the rate of sampling is
   high compared to reproductive rates.}},
  address                    = {{111 RIVER ST, HOBOKEN 07030-5774, NJ USA}},
  affiliation                = {{Skold, M (Reprint Author), Stockholm Univ, Dept Math, SE-10691 Stockholm, Sweden. Skoeld, Martin, Stockholm Univ, Dept Math, SE-10691 Stockholm, Sweden. Knape, Jonas, Swedish Univ Agr Sci, Dept Ecol, Box 7044, SE-75007 Uppsala, Sweden.}},
  article-number             = {{e02215}},
  author-email               = {{mskold@math.su.se}},
  da                         = {{2019-05-23}},
  doc-delivery-number        = {{GJ8LB}},
  doi                        = {{10.1002/ecs2.2215}},
  funding-acknowledgement    = {{Swedish Research Council VR {[}2013: 05204, 621-2012-4076]}},
  funding-text               = {{We thank SANParks for kindly sharing the data used in the study. We acknowledge financial support from the Swedish Research Council VR (Grant 2013: 05204 to MS and Grant 621-2012-4076 to JK). MS conceived the main idea and designed methodology, and MS and JK wrote the manuscript.}},
  issn                       = {{2150-8925}},
  journal-iso                = {{Ecosphere}},
  keywords                   = {{animal population dynamics; logistic function; reproductive rate; state-space model}},
  keywords-plus              = {{ESTIMATING DENSITY-DEPENDENCE; TIME-SERIES; OBSERVATION ERROR; PROCESS NOISE; GROWTH; CLIMATE; TEMPERATURE; RAINFALL; CENSUS; TRENDS}},

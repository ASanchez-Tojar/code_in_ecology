  da                         = {{2019-05-23}},
  doc-delivery-number        = {{HU7AZ}},
  doi                        = {{10.1098/rspb.2019.0365}},
  eissn                      = {{1471-2954}},
  funding-acknowledgement    = {{EPSRC}},
  funding-text               = {{M.F. gratefully acknowledges a post-graduate studentship funded by the EPSRC.}},
  issn                       = {{0962-8452}},
  journal-iso                = {{Proc. R. Soc. B-Biol. Sci.}},

  issn                       = {{0962-8452}},
  journal-iso                = {{Proc. R. Soc. B-Biol. Sci.}},
  keywords                   = {{social dilemmas; intelligence; kin selection}},
  keywords-plus              = {{SOCIAL BRAIN HYPOTHESIS; HUMAN-EVOLUTION; PUBLIC-GOODS; POPULATIONS; SELECTION; BEHAVIOR; INTELLIGENCE; SIZE; INTERDEPENDENCE; GENERATIONS}},
